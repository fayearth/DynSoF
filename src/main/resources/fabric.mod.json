{
  "schemaVersion": 1,
  "id": "dynsof",
  "version": "${version}",
  "name": "DynSoF",
  "description": "Adds dynamic sound filters such as reverb in caves or ravines, muffled underwater sounds, dampens sounds behind walls and many more. All configurable.",
  "authors": [
    "andre111",
    "MikenRafter",
	  "Fayearth"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/dynamic-sound-filters",
    "sources": "https://bitbucket.org/andre111/dynamicsoundfilters/src",
    "issues": "https://github.com/fayearth/DynSoF/issues"
  },
  "icon": "icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "me.andre111.dynamicsf.DynamicSoundFilters"
    ],
    "modmenu": [
      "me.andre111.dynamicsf.config.ConfigScreen"
    ]
  },
  "mixins": [
    "me.andre111.dynamicsf.mixin.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.0",
    "fabric": "*",
    "minecraft": "1.17.1",
    "java": ">=16"
  },
  "suggests": {
    "modmenu": "*"
  }
}
